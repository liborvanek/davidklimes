{
  "name": "david-klimes",
  "description": "David Klimeš website –  Sapper/Svelte and PostCSS preprocessing with Tailwind CSS",
  "scripts": {
    "eslint": "eslint",
    "eslint:fix": "eslint --fix ./*.js ./src/*.js ./src/routes/**/*.svelte ./src/components/**/*.svelte",
    "dev": "sapper dev",
    "build": "cross-env NODE_ENV=production PORT=4001 sapper build --legacy",
    "prod": "npm run build",
    "export": "cross-env NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "@tailwindcss/typography": "0.4.0",
    "autoprefixer": "10.2.3",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "date-fns": "2.16.1",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1",
    "helmet": "4.4.1",
    "mongoose": "5.11.13",
    "node-cron": "2.0.3",
    "node-fetch": "2.6.1",
    "p-min-delay": "3.2.0",
    "rss-parser": "3.10.0",
    "sirv": "1.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/runtime": "7.12.5",
    "@rollup/plugin-babel": "5.2.2",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "11.1.0",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/plugin-typescript": "6.0.0",
    "@tsconfig/svelte": "1.0.10",
    "@types/body-parser": "1.19.0",
    "@types/compression": "1.7.0",
    "@types/dotenv-safe": "8.1.1",
    "@types/express": "4.17.11",
    "@types/helmet": "4.0.0",
    "@types/node": "14.14.22",
    "@types/node-cron": "2.0.3",
    "@types/node-fetch": "2.5.8",
    "@types/polka": "0.5.2",
    "cross-env": "7.0.3",
    "cssnano": "4.1.10",
    "eslint": "7.18.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-svelte3": "3.0.0",
    "kleur": "4.1.4",
    "postcss": "8.2.1",
    "postcss-import": "13.0.0",
    "prettier": "2.2.1",
    "prettier-plugin-svelte": "2.1.1",
    "rollup": "2.38.0",
    "rollup-plugin-svelte": "7.0.0",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.28.10",
    "svelte": "3.31.0",
    "svelte-check": "1.0.46",
    "svelte-preprocess": "4.3.0",
    "tailwindcss": "2.0.2",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  }
