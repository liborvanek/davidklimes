{
  "name": "david-klimes",
  "description": "David Klimeš website –  Sapper/Svelte and PostCSS preprocessing with Tailwind CSS",
  "scripts": {
    "eslint": "eslint",
    "eslint:fix": "eslint --fix ./*.js ./src/*.js ./src/routes/**/*.svelte ./src/components/**/*.svelte",
    "dev": "sapper dev",
